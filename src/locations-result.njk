{% import 'macros/location-list-item.njk' as lli %}
    


{% for postal, location in locationByPostal %}

        {% if locationPostal == postal %}
                {%for l in location %}
                        {{ lli.locationListItem(l) }}
                {% endfor %}
        {% endif %}

{% endfor %}